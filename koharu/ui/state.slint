import { Image, TextBlock } from "types.slint";

export global Document {
    in-out property <Image> image;
    in-out property <[TextBlock]> text-blocks;
    in-out property <image> segment;
}

export global Viewport {
    in-out property <[Image]> images;
    in-out property <int> scale: 100;
    in-out property <bool> show-segmentation: false;
    in-out property <bool> in-progress: false;
}

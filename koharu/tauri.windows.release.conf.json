{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "identifier": "Koharu",
  "build": {
    "features": [
      "cuda"
    ]
  },
  "bundle": {
    "active": true,
    "targets": "nsis",
    "resources": [
      "../target/release/cu*.dll",
      "../target/release/nv*.dll",
      "../target/release/onnxruntime*.dll"
    ],
    "windows": {
      "signCommand": "trusted-signing-cli -e https://eus.codesigning.azure.net -a Mayo -c Hawaii -d Koharu %1"
    }
  }
}
